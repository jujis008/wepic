.container_top{
	width:1180px;
	height:48px;
	background:#fff;
	margin:10px auto 10px;
	text-align:left;
	font-family:"微软雅黑";
	}

.container_top h2{
	width:1000px;
	height:48px;
	line-height:48px;
	float:left;
	padding-left:24px;
	font-weight:normal;
	color:#000;
	font-size:20px;
	}
.container_top h2 span{
	margin-right:8px;
	}
.container_top .goBack{
	float:left;
	background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -1072px;
    border: 0 none;
    color: #000000;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
	margin-right:20px;
	margin-top:6px;
    text-align: center;
    width: 116px;
	}
	
.container_top .goBack:hover{
	background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -1104px;
	}
.container_top .goBack:active{
	background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -1136px;
	}


.user_account_info{
	width:1180px;
	height:100px;
	background:#fff;
	margin:0px auto 10px;
	text-align:left;
	}
	
.user_account_info .account_info_left{
	position:relative;
	top:0;
	left:0;
	width: 285px;
	height: 100px;
	float:left;
	}	
	
.user_account_info .account_info_left  .headImg{
	width: 60px;
	height: 54px;
    left: 20px;
    position: absolute;
    top: 15px;
	}
.user_account_info .account_info_left .headImg .headImgCon{
	width: 40px;
	height: 40px;
    left: 9px;
    position: absolute;
    top: 7px;
	/*background:url(../../../back/pubimg/images/defaultHead_80.png) 0 0 no-repeat;*/
	}
.user_account_info .account_info_left  .headImg .head{
	width: 40px;
	height: 40px;
	left: 0;
    position: absolute;
    top: 0;
	}
.user_account_info .account_info_left  .headImg .radius{
	width: 40px;
	height: 40px;
	left: 0;
    position: absolute;
    top: 0;
	}	
.user_account_info .account_info_left  .info{
	width: 225px;
	height: 54px;
    left: 80px;
    position: absolute;
    top: 10px;
	}
.user_account_info .account_info_left  .info .name{
	height: 30px;
	line-height: 30px;
	color:#000;
	font-family:"微软雅黑";
	font-size:14px;
	}
	
/*公用*/
