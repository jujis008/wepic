.container_body .div_job{
	padding:10px 20px;
    position: relative;
    top: 0;
    left: 0;
	height:30px;
	width:1140px;
	margin:10px auto 10px;
	background:#fff;
	}
.container_body .div_job .ul_job li{
	float:left;
	margin-top:8px;
	padding:0px 17px;
	font-family:"微软雅黑";
	color:#494949;
	}
.container_body .div_job .ul_job .active{
	color:#FF0000;
	}


.container_body .div_job .operation{
    position: absolute;
    top: 10px;
    right: 20px;
	width:150px;
	} 

.container_body .div_job .operation .backToPrevious {
    background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -1072px;
    border: 0 none;
    color: #000000;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
    margin-left: 12px;
    padding-bottom: 2px;
    text-align: center;
    width: 116px;
	}

.container_body .div_job .operation .backToPrevious:hover{
	background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -1104px;
	}
.container_body .div_job .operation .backToPrevious:active{
	background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -1136px;
	}

.container_body .div_container{
    position: relative;
    top: 0;
    left: 0;
	height:560px;
	width:1180px;
	margin:0 auto;
	}

.container_body .div_container .left_Con{
	width:400px;
	height:548px;
	padding:3px 2px;
	float:left;
	border:1px solid #e9e9e9;
	background:#fff;
	}
.container_body .div_container .left_Con img{
	width:400px;
	height:548px;
	}
.container_body .div_container .middle_Con{
	width:400px;
	height:554px;
	float:left;
	border:1px solid #e9e9e9;
	border-left:0;
	background:#fff;
	}
.container_body .div_container .middle_Con table{
	width:400px;
	min-height:240px;
	}
.container_body .div_container .middle_Con table .td_one{
	width:73px;
	height:36px;
	}
.container_body .div_container .middle_Con table .td_two{
	width:300px;
	text-align:left;
	}
.container_body .div_container .right_Con{
	width:359px;
	height:554px;
	float:left;
	text-align:left;
	margin-left:13px;
	}
	
	
	
	

/*上一个下一个*/
.container_body .div_container .right_Con .preNext{
	width:359px;
	height:50px;
	background:#ffffff;
	margin-bottom:10px;
	}
.container_body .div_container .right_Con .preNext .pre{
	width:178px;
	height:50px;
	float:left;
	border-right:1px solid #dfdfdf;
	}
.container_body .div_container .right_Con .preNext .next{
	width:178px;
	height:50px;
	float:left;
	}
.container_body .div_container .right_Con .preNext  a {
    color: #A0A0A0;
    height: 25px;
    left: 0px;
    margin-left: 55px;
    margin-top: 15px;
    position: relative;
    top: 0px;
    width: 75px;
    display: inline-block;
}
.container_body .div_container .right_Con .preNext .pre a i {
    height: 20px;
    left: 0;
    position: absolute;
    top: 0;
    width: 20px;
}
.container_body .div_container .right_Con .preNext .pre a span {
    position: absolute;
    right: 0;
    top: 2px;
}
.container_body .div_container .right_Con .preNext .next a i {
    height: 20px;
    right: 0;
    position: absolute;
    top: 0;
    width: 20px;
}
.container_body .div_container .right_Con .preNext .next a span {
    position: absolute;
    left: 0;
    top: 2px;
}
.container_body .div_container .right_Con .preNext .pre a i {
    background: url(../../../public/images/preNext.png) no-repeat 0 0 ;
	}
.container_body .div_container .right_Con .preNext .pre a:hover i {
    background: url(../../../public/images/preNext.png) no-repeat 0 -25px ;
	}
	
.container_body .div_container .right_Con .preNext .next a i {
    background: url(../../../public/images/preNext.png) no-repeat -25px 0px ;
	}
.container_body .div_container .right_Con .preNext .next a:hover i {
    background: url(../../../public/images/preNext.png) no-repeat -25px -25px;
	}
