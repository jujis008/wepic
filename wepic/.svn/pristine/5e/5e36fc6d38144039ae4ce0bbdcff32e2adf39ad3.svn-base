/*实名审核*/
#viewBigPic{
	position:relative;
	top:0;
	left:0;
	display:none;
	}
#viewBigPic img{
	max-height:800px;
	max-width:800px;
	}
#viewBigPic a {
    background:url(../../../public/images/closeBox.png) 0 0 no-repeat;
    height: 58px;
    position: absolute;
    right: 0;
    top: 0;
    width: 58px;
	cursor:pointer;
	filter:alpha(opacity=50);    
    opacity: 0.2;
	}
#viewBigPic a:hover{
	filter:alpha(opacity=100);  
    opacity: 1;
	background:url(../../../public/images/closeBox.png) no-repeat 0 -58px;
	}
#viewBigPic a:active{
	filter:alpha(opacity=100);    
    opacity: 1;
	background:url(../../../public/images/closeBox.png) no-repeat 0 -116px;
	}