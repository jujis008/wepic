#appointContent .divMainList .filterWrapperSortRR .textMoney{
	background: #FCFCFC;
    border: 1px solid #E8E8E6;
    border-radius: 3px;
    color: #696969;
    font-family: "微软雅黑";
    font-size: 14px;
    height: 20px;
    padding: 5px 10px;
    width: 60px;
	}
#appointContent .divMainList .filterWrapperSortRR .textsure{
	position:absolute;
	bottom:0px;
	right:20px;
	display:none;
	}
