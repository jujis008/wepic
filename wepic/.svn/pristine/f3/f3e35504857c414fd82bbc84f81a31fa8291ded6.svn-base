#divReleaseRecord{
    position: relative;
    top: 0;
    left: 0;
    margin:0 auto;
    width: 1180px;
	max-height:1346px;
	margin-top:10px;
	color:#848484;
	margin-bottom:40px;
	}

#divReleaseRecord .RecordHead{
	position: relative;
    top: 0;
    left: 0;
	font-size:12px;
	font-family:"宋体";
    width: 1180px;
	height:55px;
	background:#ffffff;
	z-index:10;
	}
#divReleaseRecord .RecordHead .tableHead{
	position: relative;
    top: 10px;
	height:30px;
	width:1180px;
    left: 0;
	text-align:left;
	}
#divReleaseRecord .RecordHead .tableHead .td1{
	width:53px;
	text-align:center;
	}
#divReleaseRecord .RecordHead .tableHead .td2{
	width:150px;
	}
#divReleaseRecord .RecordHead .tableHead .td5{
	width:500px;
	vertical-align:bottom;
	}
#divReleaseRecord .RecordHead .tableHead .releaseNewinput{
	background:url(../../../..//public/images/buttonGroup01.png) no-repeat 0 -1168px;
    border: 0;
    color: #ffffff;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
	margin-left:12px;
    padding-bottom: 2px;
    text-align: center;
    width: 116px;
	}
#divReleaseRecord .RecordHead .tableHead .releaseNewinput:hover{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1200px;
	}
	
	
	
	
#divReleaseRecord .content{
	position: relative;
    top: 0;
    left: 0;
	font-size:12px;
	font-family:"宋体";
    width: 1180px;
	min-height:310px;
	margin-top:10px;
	}
#divReleaseRecord .content .contentList{
	position: relative;
    top: 0;
    right: 0;
	width:783px;
	min-height:310px;
	max-height:1346px;
	}
#divReleaseRecord .content .contentList .divUL{
	margin-top:-20px;
	}	
#divReleaseRecord .content .contentList .divUL .active .divLi{
	background:#ffffff;
	}
#divReleaseRecord .content .contentList .divLi{
	position: relative;
    top: 0;
    right: 0;
	border:1px solid #e8e8e8;
	border-left:2px solid #0095ea;
	width:783px;
	height:76px;
	text-align:left;
	margin-top:34px;
	background:#f9f9f9;
	}
	
#divReleaseRecord .content .contentList .divLi .head{
	position: relative;
    top: 0;
    right: 0;
	border-bottom:1px solid #e8e8e8;
	width:783px;
	height:44px;
	}
#divReleaseRecord .content .contentList .divLi .head .name{
	color:#232323;
	font-family:"微软雅黑";
	font-size:20px;
	padding-left:16px;
	height:44px;
	line-height:44px;
	
	}	
	
#divReleaseRecord .content .contentList .divLi .head .offer{	
	display:inline-block;
	width:46px;
	text-align:center;
	height:18px;
	line-height:18px;
	background:#febc44;
	color:#ffffff;
	}
	
#divReleaseRecord .content .contentList .divLi .head .namefin{
	color:#9b9b9b;
	font-family:"微软雅黑";
	font-size:20px;
	padding-left:16px;
	height:44px;
	line-height:44px;
	
	}	
#divReleaseRecord .content .contentList .divLi .head .namefin:hover{
	color:#232323;
	}	
#divReleaseRecord .content .contentList .divLi .head .offerfin{	
	display:inline-block;
	width:46px;
	text-align:center;
	height:18px;
	line-height:18px;
	background:#cdcdcd;
	color:#ffffff;
	}
#divReleaseRecord .content .contentList .divLi .head .screen{	
	display:inline-block;
	min-width:65px;
	text-align:center;
	height:18px;
	line-height:18px;
	background:#cdcdcd;
	color:#ffffff;
	padding-left:5px;
	padding-right:5px;
	}
	
#divReleaseRecord .content .contentList .divLi .head .divListTitleIco{
	height: 20px;
    right: 0px;
    position: absolute;
    top: 22px;
    width: 180px;
	}	
#divReleaseRecord .content .contentList .divLi .head .divListTitleIco a{
	padding-left:20px;
	margin-right:5px;
	color:#9b9b9b;
	}
#divReleaseRecord .content .contentList .divLi .head .divListTitleIco .divListTitleIcoOne{
	background:url(../../../../public/images/11.png) 0 -2px no-repeat;
	}
#divReleaseRecord .content .contentList .divLi .head .divListTitleIco .divListTitleIcoTwo{
	background:url(../../../../public/images/11.png) no-repeat 0 -21px;
	}
#divReleaseRecord .content .contentList .divLi .head .divListTitleIco .divListTitleIcoThree{
	background:url(../../../../public/images/11.png) no-repeat 0 -40px;
	}	
	
#divReleaseRecord .content .contentList .divLi .num{
	position: relative;
    top: 0;
    right: 0;
	width:783px;
	height:30px;
	line-height:30px;
	}	
	
#divReleaseRecord .content .contentList .divLi .num .projectNum{
	padding-left:16px;
	}	
	
#divReleaseRecord .content .contentList .divLi .num .divNumTime{
	height: 30px;
    right: 0px;
	top:0;
    position: absolute;
    min-width: 250px;
	text-align:right;
	}		
#divReleaseRecord .content .contentList .divLi .num .divNumTime .time{
	margin-right:5px;
	}	
#divReleaseRecord .content .contentList .divLi .num .divNumTime .zero{
	display:inline-block;
	width:103px;
	height:33px;
	line-height:33px;
	background:#6cb3dd;
	font-family:"微软雅黑";
	font-size:13px;
	color:#ffffff;
	text-align:center;
	}	
#divReleaseRecord .content .contentList .divLi .num .divNumTime .have{
	display:inline-block;
	min-width:93px;
	height:33px;
	line-height:33px;
	background:#6cb3dd;
	font-family:"微软雅黑";
	font-size:13px;
	padding-left:5px;
	padding-right:5px;
	color:#ffffff;
	text-align:center;
	}	
	
	
	
	
		
#divReleaseRecord .content .contentAll{
	position: absolute;
    top: 0;
    right: 0;
	width:385px;
	height:310px;
	background:#ffffff;
	border:1px solid #e8e8e8;
	color:#a7a7a7;
	font-family:"微软雅黑";
	text-align:left;
	}	
		
#divReleaseRecord .content .contentAll .head{
	width:385px;
	height:44px;
	border-bottom:1px solid #e8e8e8;
	color:#000000;
	}
#divReleaseRecord .content .contentAll .head h2{
	font-weight:normal;
	height:44px;
	line-height:44px;
	padding-left:15px;
	}
#divReleaseRecord .content .contentAll .goodbag{
	width:384px;
	height:83px;
	border-bottom:1px solid #e8e8e8;
	color:#000000;
	}
#divReleaseRecord .content .contentAll .goodbag .evaluate{
	float:left;
	height:83px;
	width:95px;
	text-align:center;
	border-right:1px solid #e8e8e8;
	}
#divReleaseRecord .content .contentAll .goodbag .evaluatelast{
	border-right:0;
	
	}
#divReleaseRecord .content .contentAll .goodbag .evaluate .evnum{
	height:50px;
	line-height:50px;
	font-family:"黑体";
	font-size:26px;
	color:#646464;
	}
#divReleaseRecord .content .contentAll .goodbag .evaluate .divp .goodp{
	height:24px;
	background:url(../../../pubimg/images/googbagbj.png) 0 0 no-repeat;
	padding-left:26px;
	}
#divReleaseRecord .content .contentAll .goodbag .evaluate .divp .badp{
	height:24px;
	background:url(../../../pubimg/images/googbagbj.png) 0 -22px no-repeat;
	padding-left:26px;
	}

#divReleaseRecord .content .contentAll .detail{
	width:384px;
	height:76px;
	border-bottom:1px solid #e8e8e8;
	}
#divReleaseRecord .content .contentAll .expend{
	border-bottom:0;
	}
#divReleaseRecord .content .contentAll .detail table{
	border-collapse:0;
	text-align:center;
	font-size:14px;
	}
#divReleaseRecord .content .contentAll .detail table tr{
	height:38px;
	}
#divReleaseRecord .content .contentAll .detail table .td1{
	width:104px;
	}
#divReleaseRecord .content .contentAll .detail table .td2{
	width:92px;
	color:#636363;
	}
#divReleaseRecord .content .contentAll .detail table .td3{
	width:90px;
	text-align:left;
	}
#divReleaseRecord .content .contentAll .detail table .td4{
	width:90px;
	color:#636363;
	}
#divReleaseRecord .content .contentAll .detail table .tr2 td{
	vertical-align:top;
	}



	/*编辑*/

#divReleaseRecord .content .contentList .divLi .edit{
	display:none;
	position: absolute;
    right: 0;
    top: 0;
    width: 166px;
	height:38px;
	}
#divReleaseRecord .content .contentList .divUL .active .edit{
	display:block;
	}
#divReleaseRecord .content .contentList .divLi .edit .bj{
	position: absolute;
    left: 0;
    top: 0;
    width: 166px;
	height:38px;
	background:#000000;
	filter:alpha(opacity=70);    /* IE 4-7 */     
	opacity: 0.7;              /* Firefox, Safari(WebKit), Opera */
	}
/*#divReleaseRecord .content .contentList .divLi .edit .ed a:hover{
	color:#0068b7;
	}*/
#divReleaseRecord .content .contentList .divLi .edit .ed{
	position: absolute;
    left: 0;
    top: 0;
    width: 166px;
	height:38px;
	}
#divReleaseRecord .content .contentList .divLi .edit .ed .del{
	position: absolute;
    left: 26px;
    top: 10px;
    width: 50px;
	height:20px;
	background:url(../../../pubimg/images/editdel.png) 0 -66px no-repeat;
	color:#ffffff;
	font-size:13px;
	font-family:"宋体";
	padding-left:24px;
	}
#divReleaseRecord .content .contentList .divLi .edit .ed .edi{
	position: absolute;
    right: 00px;
    top: 10px;
    width: 50px;
	height:20px;
	background:url(../../../pubimg/images/editdel.png) 0 -44px no-repeat;
	color:#ffffff;
	font-size:13px;
	font-family:"宋体";
	padding-left:24px;
	}
	
	/*编辑*/

.ppointPagingthis{
	position:relative;
	left:286px;
	top:20px;
	
	width:500px;
	height:60px;
	}
.ppointPagingthis .ppointPaging{
	background:#F4F4F4;
	}
.ppointPagingthis .ppointPaging .ppointPagingN {
	border:1px solid #F4F4F4;
	}
.ppointPagingthis .ppointPaging .ppointPagingN:hover{
	border:1px solid #b7b9b6;
	}