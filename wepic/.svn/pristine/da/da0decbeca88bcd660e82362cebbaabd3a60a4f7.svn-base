			tableTR(); 
		
		});
function tableTR(obj){
	var allTR = $(".trCon");
	$(allTR).each(function(index, element) {
        $(this).click(
		function(){
			window.location = 'InvoicedListDetail.html';
			}
		);
    });
	
	}


function openPopUpBoxNoPass(obj){
	openPopUpBox('noPassReason');
	$("#hiddenPop").val($(obj).parents("tr").attr('id'));
	
	}
	


function gobackList(){
	$(".withdrawDeposit_cont_char").hide();
	$("#txtBeginDate").val('');
	$("#txtEndDate").val('');
	
}

function query(){
	var startTime = $("#txtBeginDate").val();
	var endTime = $("#txtEndDate").val();
	
	
	var startTimeV = parseInt(trimH(trim(startTime)));
	var endTimeV = parseInt(trimH(trim(endTime)));
	
	
	
	if(startTime == null || startTime == ''){
		alert("请选择开始日期");
		return;
		}
	if(endTime == null || endTime == ''){
		alert("请选择截止日期");
		return;
		}
	if(endTimeV < startTimeV){
		alert("请重新选择日期");
		return;
		}
		
	if(endTimeV >= startTimeV){
		$(".withdrawDeposit_cont_char").show();
		return;
		}
	
	}


</script>

