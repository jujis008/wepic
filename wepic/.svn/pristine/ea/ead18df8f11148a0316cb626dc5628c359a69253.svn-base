/*右边模块*/



/*专辑移动到*/
#moveTozj{
	display:none;
	width:480px;
    background:#ffffff;
	text-align:left;
	}

#moveTozj .top{
    background:#F6F6F6;
    border-bottom: 1px solid #E3E3E3;
    height: 57px;
    left: 0;
    position: relative;
    top: 0;
    width: 480px;
}
#moveTozj .top h2 {
    color: #585858;
    font-family: "微软雅黑";
    font-size: 22px;
    font-weight: normal;
    height: 58px;
    line-height: 58px;
    padding-left: 30px;
	text-align:left;
	}

#moveTozj .top a {
    background:url(../../../public/images/closeBox.png) 0 0 no-repeat;
    height: 58px;
    position: absolute;
    right: 0;
    top: 0;
    width: 58px;
	}
#moveTozj .top a:hover{
	background:url(../../../public/images/closeBox.png) no-repeat 0 -58px;
	}
#moveTozj .top a:active{
	background:url(../../../public/images/closeBox.png) no-repeat 0 -116px;
	}
#moveTozj .zhuanji{
    min-height: 285px;
    left: 0;
    position: relative;
    top: 0;
    width: 410px;
	padding-left:35px;
	padding-right:35px;
}
#moveTozj .zhuanji li {
    border-bottom: 1px solid #F0F0F0;
    height: 42px;
    left: 0;
    position: relative;
    top: 0;
    width: 415px;
}
#moveTozj .zhuanji li:hover,
#moveTozj .zhuanji .active {
	background:#fafafa;
	}
#moveTozj .zhuanji .special{
	position: relative;
    top: 0;
    left: 0;
	height:42px;
    width: 415px;
	}
#moveTozj .zhuanji .special .tone{
	position: absolute;
    bottom: 8px;
    left: 8px;
	width:15px;
	height:15px;
	}
	
#moveTozj .zhuanji .special .tone1{
	background:#F93;
	}
#moveTozj .zhuanji .special .tone2{
	background:#09F;
	}
#moveTozj .zhuanji .special .tone3{
	background:#6C6;
	}
#moveTozj .zhuanji .special .tone4{
	background:#090;
	}
#moveTozj .zhuanji .special .tone5{
	background:#FF6;
	}
#moveTozj .zhuanji .special .tone6{
	background:#000;
	}
#moveTozj .zhuanji .special .tone7{
	background:#0FF;
	}
#moveTozj .zhuanji .special .tone8{
	background:#033;
	}
#moveTozj .zhuanji .special .tone9{
	background:#03F;
	}
#moveTozj .zhuanji .special .tone10{
	background:#CF0;
	}
#moveTozj .zhuanji .special .name{
	position: absolute;
    bottom: 8px;
    left: 35px;
	width:360px;
	height:15px;
	font-family:"微软雅黑";
	color:#494949;
	}
#moveTozj .zhuanji .special .click{
	position: absolute;
    top: 5px;
    right: 0px;
	width:32px;
	height:32px;
	background:url(../../../public/images/checked2.png) 0 0 no-repeat;
	}
#moveTozj .zhuanji .divAddSpecial{
	margin-top:26px;
	margin-left:5px;
	margin-bottom:20px;
	}
#moveTozj .zhuanji .divAddSpecial a{
	
	font-family:"微软雅黑";
	color:#0267b7;
	}
	
	
	
	
	

#moveTozj .tijaio{
    width: 480px;
	height: 100px;
	text-align:center;
	}
#moveTozj .tijaio .reset {
    background:url(../../../public/images/buttonGroup01.png) no-repeat 0 -836px;
    border: 0 none;
    color: #242424;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 17px;
    height: 36px;
    padding-bottom: 2px;
    padding-left: 0;
    width: 143px;
	letter-spacing: 15px;
    padding-left: 20px;
	}	
#moveTozj .tijaio .reset:hover{
	background: url(../../../public/images/buttonGroup01.png) no-repeat 0 -872px;
	}
#moveTozj .tijaio .reset:active{
	background: url(../../../public/images/buttonGroup01.png) no-repeat 0 -908px;
	}
#moveTozj .tijaio .sub {
    background: url(../../../public/images/buttonGroup01.png) no-repeat 0 -718px;
    border: 0 none;
    color: #FFFFFF;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 17px;
    height: 36px;
    margin-left: 20px;
    padding-bottom: 2px;
    padding-left: 0;
    width: 143px;
	letter-spacing: 15px;
    padding-left: 20px;
	}
#moveTozj .tijaio .sub:hover{
	background: url(../../../public/images/buttonGroup01.png) no-repeat 0 -754px;
	}
#moveTozj .tijaio .sub:active{
	background: url(../../../public/images/buttonGroup01.png) no-repeat 0 -790px;
	color: #D6D6D6;
	text-shadow: 1px 1px 1px black;
	}
/*专辑移动到*/