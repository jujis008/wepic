.TW_chooseBlank .topUp_submit{
	width:580px;
	height:80px;
	margin:40px auto 20px;
	text-align:center;
	padding-bottom:20px;
	padding-top:40px;
	border-top: 1px solid #E8E8E8;
	}


.TW_chooseBlank .topUp_submit .recharge{
    background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1168px;
    border: 0;
    color: #ffffff;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
    text-align: center;
    width: 116px;
	}

.TW_chooseBlank .topUp_submit .recharge:hover{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1200px;
	}
.TW_chooseBlank .topUp_submit .recharge:active{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1136px;
	color: #000000;
	}


.TW_chooseBlank .topUp_submit .submit_cancel{
	margin-top:20px;
	}
.TW_chooseBlank .topUp_submit a {
    color: #0084C7;
    text-decoration: underline;
	}







	
.TW_chooseBlank .withdrawIntro{
	border-top: 1px solid #E8E8E8;
	padding:25px;
	}
.TW_chooseBlank .withdrawIntro h2{
    color: #585858;
    font-family: "微软雅黑";
    font-size: 14px;
	font-weight:normal;
	height:30px;
	line-height:30px;
	}
.TW_chooseBlank .withdrawIntro .child{
	padding:0 30px;
	line-height:22px;
	}










/*添加提现银行卡*/

.add_withdraw_deposit{
	width:560px;
	height:550px;
	background:#ffffff;
	display:none;
	}
.add_withdraw_deposit .top{
    background:#F6F6F6;
    border-bottom: 1px solid #E3E3E3;
    height: 57px;
    left: 0;
    position: relative;
    top: 0;
    width:560px;
	}
.add_withdraw_deposit .top h2 {
    color: #585858;
    font-family: "微软雅黑";
    font-size: 22px;
    font-weight: normal;
    height: 58px;
    line-height: 58px;
    padding-left: 30px;
	text-align:left;
	}

.add_withdraw_deposit .top a {
    background:url(../../../../public/images/closeBox.png) 0 0 no-repeat;
    height: 58px;
    position: absolute;
    right: 0;
    top: 0;
    width: 58px;
	}
.add_withdraw_deposit .top a:hover{
	background:url(../../../../public/images/closeBox.png) no-repeat 0 -58px;
	}
.add_withdraw_deposit .top a:active{
	background:url(../../../../public/images/closeBox.png) no-repeat 0 -116px;
	}



	
.add_withdraw_deposit .table_con{
    height: 280px;
    width: 560px;
	margin-top:30px;
	}
.add_withdraw_deposit .table_con .td_one{
    width: 145px;
	padding-right:15px;
	text-align:right;
	height:50px;
	}
.add_withdraw_deposit .table_con .td_two{
	text-align:left;
	}
.add_withdraw_deposit .table_con .td_two .open_name,
.add_withdraw_deposit .table_con .td_two .input_remark{
    background: #FCFCFC;
    border: 1px solid #E8E8E6;
    border-radius: 3px;
    color: #696969;
    font-family: "微软雅黑";
    font-size: 14px;
    height: 20px;
    padding: 5px 10px;
    width: 114px;
	}
.add_withdraw_deposit .table_con .td_two .open_blank,
.add_withdraw_deposit .table_con .td_two .blank_number{
    background: #FCFCFC;
    border: 1px solid #E8E8E6;
    border-radius: 3px;
    color: #696969;
    font-family: "微软雅黑";
    font-size: 14px;
    height: 20px;
    padding: 5px 10px;
    width: 254px;
	}


.add_withdraw_deposit .submit{
	margin-top:50px;
	}
.add_withdraw_deposit .submit .save {
    background: url(../../../../public/images/buttonGroup01.png) no-repeat 0 -718px;
    border: 0;
    color: #FFFFFF;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 17px;
    height: 36px;
    width: 143px;
	letter-spacing: 15px;
    padding-left: 20px;
	}
.add_withdraw_deposit .submit .save:hover{
	background: url(../../../../public/images/buttonGroup01.png) no-repeat 0 -754px;
	}
.add_withdraw_deposit .submit .save:active{
	background: url(../../../../public/images/buttonGroup01.png) no-repeat 0 -790px;
	color: #D6D6D6;
	text-shadow: 1px 1px 1px black;
	}


.add_withdraw_deposit .tr_remind{
	heigth:20px;
	text-align:left;
	color:#ff0000;
	display:none;
	}

.add_withdraw_deposit .table_con .dropDownBox{
    height: 36px;
	padding: 0 40px 0 0;
	}

.add_withdraw_deposit .table_con .dropDownBox .dropDownBoxR {
    height: 26px;
	}

.add_withdraw_deposit .table_con .dropDownBox .dropDownBoxR i {
    top: 11px;
	}
	

.add_withdraw_deposit .table_con .dropDownBoxValue {
	width: 210px;
	top: 38px;
	}

.add_withdraw_deposit .table_con .dropDownBoxValue div{
    width: 210px;
	margin:0;
	padding-left:12px;
	}
.add_withdraw_deposit .table_con .dropDownBoxValue div:hover{
	background-color:#f0f0f0;
	}
	
	
	
	