.TW_chooseBlank{
	width:870px;
	height:700px;
	background:#fff;
	margin:10px auto 50px;
	text-align:left;
	font-family:"宋体";
	}




.TW_chooseBlank .topUp_top{
	width:870px;
	height:78px;
	position:relative;
	top:0;
	left:0;
	text-align:center;
	margin:0px auto;
	border-bottom: 1px solid #E8E8E8;
	}
.TW_chooseBlank .topUp_top a{
	position:absolute;
	right:20px;
	bottom:10px;
	color: #727272;
    text-decoration: underline;
	}
.TW_chooseBlank .topUp_top h2{
	font-weight:normal;
	color:#000;
	font-size:24px;
	line-height:78px;
	font-family:"微软雅黑";
	}
	



.TW_chooseBlank .table_TW{
	width:580px;
	height:180px;
	margin:0px auto;
	text-align:center;
	margin-top:36px;
	}
.TW_chooseBlank .table_TW .td_one{
	width:80px;
	height:60px;
	}

.TW_chooseBlank .table_TW .td_two{
	width:500px;
	text-align:left;
	color:#5b5b5b;
	}
	
.TW_chooseBlank .table_TW .td_two .usable_balance,
.TW_chooseBlank .table_TW .td_two .yuan{
	color:#000;
	font-size:12px;
	margin-right:10px;
	font-family:"微软雅黑";
	
	}

.TW_chooseBlank .table_TW .td_two .dije{
	margin-left:15px;
	}

.TW_chooseBlank .table_TW .td_two .money_deposit{
    background: #FCFCFC;
    border: 1px solid #E8E8E6;
    border-radius: 3px;
    color: #696969;
    font-family: "微软雅黑";
    font-size: 14px;
    height: 20px;
    padding: 5px 10px;
    width: 110px;
	margin-right:10px;
	}


.TW_chooseBlank .table_TW .td_two .addCard {
    color: #0084C7;
    text-decoration: underline;
	margin-left: 40px;
	margin-top:10px;
	display: inline-block;
    margin-top: 15px;
	}

.TW_chooseBlank .table_TW .td_two .divdropDown{
	width:380px;
	cursor: pointer;
	}
.TW_chooseBlank .table_TW .td_two .dropDownBox{
	width:100%;
    height: 36px;
	padding: 0 30px 0 0;
	}
.TW_chooseBlank .table_TW .td_two .dropDownBox div,
.TW_chooseBlank .table_TW .td_two .selectBlankList .selectblankDiv div{
	float:left;
	}	
.TW_chooseBlank .table_TW .td_two .dropDownBox .blankValue,
.TW_chooseBlank .table_TW .td_two .selectBlankList .blankValue{
    height: 36px;
	line-height:36px;
	font-size:14px;
	width:120px;
	margin-left:10px;
	}
.TW_chooseBlank .table_TW .td_two .dropDownBox .tailNumber,
.TW_chooseBlank .table_TW .td_two .selectBlankList .tailNumber{
    height: 36px;
	line-height:36px;
	font-family:'宋体';
	}



.TW_chooseBlank .table_TW .td_two .selectBlankList{
    background:  #FFFFFF;
    border: 1px solid #E8E8E6;
	border-top:0;
    display: none;
    left: 0;
    position: absolute;
    top: 38px;
	color: #535353;
    font-family: "微软雅黑";
    width: 100%;
	min-height:100px;
	overflow-y:auto;
	overflow-x:hidden;
	}
.TW_chooseBlank .table_TW .td_two .selectBlankList .selectblankDiv{
	width:100%;
	height:36px;
	}
.TW_chooseBlank .table_TW .td_two .selectBlankList .selectblankDiv:hover{
	color:#000000;
	background:#F0F0F0;
	}

	
.TW_chooseBlank .table_TW .td_two .dropDownBox .dropDownBoxR {
    height: 26px;
	}
.TW_chooseBlank .table_TW .td_two .dropDownBox .dropDownBoxR i {
    top: 11px;
	}
	
.TW_chooseBlank .table_TW .td_two .dropDownBoxValue {
	width: 210px;
	top: 38px;
	}
.TW_chooseBlank .table_TW .td_two .dropDownBoxValue div{
    width: 210px;
	margin:0;
	padding-left:12px;
	}
.TW_chooseBlank .table_TW .td_two .dropDownBoxValue div:hover{
	background-color:#f0f0f0;
	}


































