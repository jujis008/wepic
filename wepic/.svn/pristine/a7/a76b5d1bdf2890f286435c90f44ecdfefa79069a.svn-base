/*导航 搜索*/
	
.type-choose{
    position: relative;
	top:0;
	left:0;
	float:left;
	width: 110px;
	}

.type-choose .v-choose{
	background:#ffffff;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 14px;
	color: #626262;
	width:64px;
	display:inline-block;
    height: 20px;
    line-height: 18px;
	padding:5px 26px 5px 20px;
    left: 0;
    position: relative;
    top: 0;
	border-radius:20px 0 0 20px;
	}
.type-choose .v-choose .v-chooseS{
	width:60px;
    height: 20px;
	display:block;
	}
.type-choose .v-choose .v-chooseR{
    height: 10px;
    width: 10px;
    position: absolute;
    right: 5px;
    top: 13px;
    background:url(../images/select_bi.png) 0 0 no-repeat;
	}

.v-choose:hover{
	background:#fffdf1;
	}
.v-choose:hover .v-chooseR{
	background:url(../images/select_bi.png) no-repeat 0 -10px;
	}	




.s-choose{
    background:  #FFFFFF;
    border: 1px solid #E8E8E6;
	border-top:0;
    display: none;
	text-align:left;
    right: 0;
    position: absolute;
    top: 30px;
    width: 96px;
	max-height:300px;
	overflow-y:auto;
	overflow-x:hidden;
	box-shadow: 3px 3px 4px #ababab;
 	/*这是对Safari和Chrome浏览器而设置的*/
 	-moz-box-shadow: 3px 3px 4px #9d9d9d;
 	/*这是专对火狐浏览器的支持而设置的*/
 	-webkit-box-shadow: 3px 3px 4px #9d9d9d;
 
 	/* For IE 8 */
 	-ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#9d9d9d')";
 	/* For IE 5.5 - 7 */
 	filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#9d9d9d');
	}
.s-choose .v-drop{
	cursor:pointer;
	display:block;
	color:#626262;
	width:106px;
	height:25px;
	line-height:22px;
	padding-left:10px;
	overflow:hidden;
	}	
.s-choose .v-drop:hover{
	color:#9c5454;
	background:#F0F0F0;
	}

