/*公共元素控制*/
*{margin: 0;padding: 0;}
body{
	margin:0;
	padding:0;
	text-align:center;
	background-color:#FFFFFF;
	color:#848484;
  	font-family:'微软雅黑' ,'宋体',Arial, Verdana ;
  	font-size:12px;
	behavior: url(../public/css/hover.htc);		/*解决ie6下hover不起作用等兼容问题*/
	}
img{border:0;}
ul{list-style:none;}
a{text-decoration:none;}
table{border:0;border-collapse:collapse;border-spacing:0;}


/* 修正IE6振动bug */
*html,*html body{
	background-image:url(about:blank);
	background-attachment:fixed;
	}
#container{
	position:relative;
	width:100%;
	margin:0 auto;
	background:#f4f4f4;
	}
	
	

.clear{clear:both;}
/*公共元素控制*/



/*------------------------头部信息------------------------------------*/
.wp-top{
	height:80px;
	}
.wp-Top{
	height:80px;
	position:fixed;
	top:0;
	left:0;
	z-index:100000;
	width:100%;
	_position:absolute;
    _bottom:auto;
    _top:expression(eval(document.documentElement.scrollTop));
	}
.wp-top .wp-TopB{
	position:fixed;
	top:0;
	left:0;
	z-index:99999;
	width:100%;
	height:80px;
	background:#000000;
	_position:absolute;
    _bottom:auto;
    _top:expression(eval(document.documentElement.scrollTop));
	filter:alpha(opacity=80);    /* IE 4-7 */
	-moz-opacity:0.8;  
	-khtml-opacity: 0.8;  
	opacity: 0.8;              /* Firefox, Safari(WebKit), Opera */
	}
