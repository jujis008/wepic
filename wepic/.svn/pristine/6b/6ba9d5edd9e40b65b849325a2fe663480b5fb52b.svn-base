.container_top{
	width:1180px;
	height:192px;
	background:#fff;
	margin:10px auto 10px;
	text-align:left;
	font-family:"宋体";
	}



.container_top .total_money{
	width:773px;
	height:192px;
	float:left;
	
	}
.container_top .total_money .character{
	padding-left:60px;
	padding-top:30px;
	height:40px;
	font-family:"微软雅黑";
	font-size:20px;
	color:#4a4a4a;
	}
.container_top .total_money .money{
	padding-left:60px;
	height:100px;
	font-family:"微软雅黑";
	font-size:70px;
	color:#e92d00;
	}




.container_top .total_detail{
	width:407px;
	height:192px;
	float:left;
	
	}
.container_top .total_detail .small_money{
	width:202px;
	height:95px;
	float:left;
	border-left:1px solid #dfdfdf;
	}
.container_top .total_detail .small_money .tit{
	padding-left:40px;
	padding-top:15px;
	height:20px;
	}	
	
.container_top .total_detail .small_money .money{
	padding-left:40px;
	color:#e82d00;
	font-size:26px;
	font-family:Arial, Helvetica, sans-serif;
	}
	
	
		
	
.container_top .total_detail .small_money_one,
.container_top .total_detail .small_money_two{
	border-bottom:1px solid #dfdfdf;
	}
.container_top .total_detail .small_money_two,
.container_top .total_detail .small_money_four{
	border-right:1px solid #dfdfdf;
	}









.container_table{
	width:1180px;
	min-height:300px;
	padding:50px 0;
	background:#fff;
	margin:0px auto 50px;
	text-align:left;
	font-family:"宋体";
	}


.container_table table{
	border:1px solid #e7e7e5;
	text-align:center;
	color:#676767;
	margin:0px auto;
	}
.container_table table td{
	padding:8px 15px;
	border:1px solid #e7e7e5;
	}
.container_table table th{
	height:30px;
	font-weight:normal;
	background:#cfe69d;
	}
	
.container_table table .name{
    color: #A01E1E;
    cursor: pointer;
    font-family: "微软雅黑";
	}	
	





