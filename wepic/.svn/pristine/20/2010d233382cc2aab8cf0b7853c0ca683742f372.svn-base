.topUp_chooseBlank{
	width:870px;
	height:840px;
	background:#fff;
	margin:10px auto 10px;
	text-align:left;
	font-family:"宋体";
	}




.topUp_chooseBlank .topUp_top{
	width:870px;
	height:78px;
	text-align:center;
	margin:0px auto;
	border-bottom: 1px solid #E8E8E8;
	}
.topUp_chooseBlank .topUp_top h2{
	font-weight:normal;
	color:#000;
	font-size:24px;
	line-height:78px;
	font-family:"微软雅黑";
	}
	
	

.topUp_chooseBlank .table_transfer{
	}


	



.topUp_chooseBlank .topUp_submit{
	width:870px;
	height:120px;
	margin:50px auto;
	text-align:center;
	}


.topUp_chooseBlank .topUp_submit .applicationDeposit {
    background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1072px;
    border: 0 none;
    color: #000000;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
	margin-right:25px;
    text-align: center;
    width: 116px;
	}

.topUp_chooseBlank .topUp_submit .applicationDeposit:hover{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1104px;
	}
.topUp_chooseBlank .topUp_submit .applicationDeposit:active{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1136px;
	}

.topUp_chooseBlank .topUp_submit .recharge{
    background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1168px;
    border: 0;
    color: #ffffff;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
    text-align: center;
    width: 116px;
	}

.topUp_chooseBlank .topUp_submit .recharge:hover{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1200px;
	}
.topUp_chooseBlank .topUp_submit .recharge:active{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1136px;
	color: #000000;
	}



