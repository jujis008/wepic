	text-align:left;
	}



.container_body .submit .inp{
	position: absolute;
    right: 0px;
    top: 30px;
	}
.container_body .submit .inp .inputup{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1072px;
    border: 0;
    color: #000000;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
	margin-right:30px;
    padding-bottom: 2px;
    text-align: center;
