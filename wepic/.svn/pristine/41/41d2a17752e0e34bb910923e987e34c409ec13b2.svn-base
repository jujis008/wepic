	position:absolute;
	right:0px;
	top:0;
	width:750px;
    min-height: 100px;
	
	}
#shootProduct .content .divChooseCon  .divInfo .productType .proType li{
	position:relative;
	left:0px;
	top:0;
	float:left;
    font-family: "微软雅黑";
	height:23px;
	line-height:23px;
	cursor:pointer;
	padding-left:10px;
	padding-right:10px;
	margin-right:10px;
	margin-top:14px;
	
	color:#000;
	border:1px solid #e8e8e8;
	background:#fcfcfc;
	}
#shootProduct .content .divChooseCon  .divInfo .productType .proType li i{
	display:none;
	position:absolute;
	right:0px;
	bottom:0;
	width:11px;
	height:10px;
	background:url(../../../pubimg/images/relprocolor.png) 0 0 no-repeat;
	}

#shootProduct .content .divChooseCon  .divInfo .productType .proType li a{
	display:none;
	position:absolute;
	top:-6px;
	right:-6px;
	width:16px;
	height:16px;
	background:url(../../../pubimg/images/background.png) -98px -110px no-repeat;
	}
#shootProduct .content .divChooseCon  .divInfo .productType .proType .active{
