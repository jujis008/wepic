#shootProduct{
    position: relative;
    top: 0;
    left: 0;
    margin:0 auto;
    width: 1180px;
	min-height:800px;
	margin-top:5px;
	margin-bottom:40px;
	background:#fff;
	}

.shootWayCon li{
	position:relative;
	left:0px;
	top:0;
	float:left;
    font-family: "微软雅黑";
	height:23px;
	line-height:23px;
	cursor:pointer;
	padding-left:10px;
	padding-right:10px;
	margin-right:10px;
	
	color:#000;
	border:1px solid #e8e8e8;
	background:#fcfcfc;
	}

.shootWayCon li i{
	display:none;
	position:absolute;
	right:0px;
	bottom:0;
	width:11px;
	height:10px;
	}
.shootWayCon .active i{
	display:block;
	}
#shootProduct .divHead{
    color: #686868;
    font-family: "微软雅黑";
    height: 60px;
    left: 0;
    position: relative;
    text-align: left;
    top: 0;
    width: 1180px;
	border-bottom:1px solid #e5e5e5;
	}


#shootProduct .divHead p{
    font-size: 20px;
    height: 60px;
    line-height: 60px;
    padding-left: 30px;
	}




#shootProduct .content {
    color: #696969;
    font-family: "宋体";
    min-height: 745px;
	width:1180px;
    left: 0;
    position: relative;
    text-align: left;
    top: 0;
	}

#shootProduct .content .title{
	margin-left:90px;
	height: 65px;
	margin-top:25px;
	
	}
#shootProduct .content .title .na {
    font-family: "微软雅黑";
    color: #000;
    font-size: 20px;
	margin-right:20px;
	}
#shootProduct .content .title .titleinput {
    background: none repeat scroll 0 0 #FCFCFC;
    border: 1px solid #E8E8E8;
    border-radius: 3px;
    font-family: "微软雅黑";
    color: #535353;
    font-size: 16px;
    height: 27px;
    padding: 5px 10px;
    width: 540px;
	}
#shootProduct .content .title .inputtext{
	border:0;
	background:none;
    color: #696969;
    font-family: "宋体";
	}	
#shootProduct .content .divChooseCon{
	position:relative;
	left:0;
	right:0;
	width: 1140px;
	margin:15px auto;
    min-height: 670px;
	}
#shootProduct .content .divChooseCon .shootBar{
	height: 188px;
	width:176px;
    font-family: "微软雅黑";
    font-size: 16px;
	border-bottom:1px solid #e3e3e3;
	}
#shootProduct .content .divChooseCon .shootBar ul li{
	/*display:inline-block;*/
	width:174px;
	height:46px;
	line-height:46px;
	text-align:center;
	cursor:pointer;
	background:#f9f9f9;
	border:1px solid #e3e3e3;
	border-bottom:0;
	color:#000;
	}
#shootProduct .content .divChooseCon .shootBar ul li:hover,
#shootProduct .content .divChooseCon .shootBar ul .active{
	background:#fff;
	color:#f00;
	}
	
	
	
	
	
/*右边模块*/

