	display:block;
	}
#shootProduct .content .divChooseCon  .divInfo .productType .proType li:hover{
	
	color:#ed9f23;
	border:1px solid #ed9f23;
	background:#fff9ef;
	}

#shootProduct .content .divChooseCon  .divInfo .productType .proType li:hover a{
	display:block;
	}

#shootProduct .content .divChooseCon  .divInfo .productType .custom{
	width: 190px;
    height: 67px;
	
	}

#shootProduct .content .divChooseCon  .divInfo .productType .custom .character{
    height: 30px;
	line-height:30px;
	}

#shootProduct .content .divChooseCon  .divInfo .productType .custom .addCustom .addinput{
    background:#FCFCFC;
    border: 1px solid #E8E8E8;
    border-radius: 3px;
    color: #535353;
    font-family: "微软雅黑";
    font-size: 14px;
    height: 17px;
    padding: 5px 10px;
    width: 99px;
	}

#shootProduct .content .divChooseCon  .divInfo .productType .custom .addCustom .surebut{
	border:0;
	background:#b5b5b5;
	color:#fff;
    font-family: "微软雅黑";
    font-size: 14px;
    width: 55px;
    height: 29px;
	cursor:pointer;
	}
/*第一行*/	






/*第二行*/	
#shootProduct .content .divChooseCon  .divInfo .shootWay{
	width: 860px;
    height: 120px;
	position:relative;
	left:0;
