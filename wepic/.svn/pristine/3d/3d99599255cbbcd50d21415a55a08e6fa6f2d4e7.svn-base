<script>
$(document).ready(function() {
	document.getElementById("formChooseWorkPop").reset();
	
	check();
	
	//checked();
});


function check(){
	
    var allCheckBox = $(".childIndustryValueCheck");
	$(allCheckBox).each(function() {
        $(this).click(
			function(){
				var thisVal = $(this).val();
				
				if($(this).is(':checked')){
					var allCheckedBox = $("#choosedUl").find("input");			//获取下面选中所有checked
					if(allCheckedBox.length < 5){
						
						var addLi = '<li><input type="checkbox" checked="checked" value="'+thisVal+'"/><span class="childIndustryValueSpan">'+thisVal+'</span></li>'
						$("#choosedUl").append(addLi);
						checked();
						}
					else{
						$(this).removeAttr("checked");
						alert("最多只可以选择5个行业领域！");
						}
					
				}
				else{
					removeLi(thisVal);	
					}
		});
    });
}

function checked(){
	
	var allCheckedBox = $("#choosedUl").find("input");			//获取下面选中所有checked
	
	$(allCheckedBox).each(function() {							//循环加click事件
        $(this).click(
			function(){
				$(this).parents("li").remove();					//删除自身
				var thisVal = $(this).val();	
				removeChecked(thisVal);							//删除列表中与之相同的
				
				}
		);
    });
}

//去掉列表中checked状态
function removeChecked(thisVal){
	var allCheckBox = $(".childIndustryValueCheck");
		$(allCheckBox).each(function() {
			var checkVal = $(this).val();
			if(thisVal ==checkVal){
				$(this).removeAttr("checked");
				}	
		});

}

//去掉下面checked
function removeLi(thisVal){
	var allCheckedBox = $("#choosedUl").find("input");			//获取下面选中所有checked
		$(allCheckedBox).each(function() {
			var checkedVal = $(this).val();		
       		if(thisVal ==checkedVal){
				$(this).parents("li").remove();					
			}
		
    });
}
</script>
