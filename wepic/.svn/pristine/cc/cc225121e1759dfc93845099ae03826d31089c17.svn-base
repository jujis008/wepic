.withdrawDeposit_record{
	width:1180px;
	min-height:500px;
	background:#fff;
	margin:5px auto 5px;
	text-align:left;
	font-family:"宋体";
	padding-bottom:50px;
	}




.withdrawDeposit_record .withdrawDeposit_record_top{
	width:100%;
	height:40px;
	text-align:left;
	margin:0px auto;
	border-bottom: 1px solid #E8E8E8;
	padding-top: 10px;
	}
	
.withdrawDeposit_record .withdrawDeposit_record_top table{
	height:35px;
	}	
.withdrawDeposit_record .withdrawDeposit_record_top table td{
	vertical-align:center;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .char{
	font-weight:normal;
	color:#000;
	font-size:18px;
	font-family:"微软雅黑";
	padding:0 20px;
	vertical-align:top;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table  .totalwith{
	padding-right:40px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table  .totalnt{
	padding-right:90px;
	}

.withdrawDeposit_record .withdrawDeposit_record_top table .timeInputStr {
    border: 1px solid #E8E8E8;
    border-radius: 3px;
    color: #535353;
    font-size: 12px;
    height: 21px;
    padding: 3px 10px;
    width: 90px;
	margin-left:5px;
	margin-right:5px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .inquire_td{
	padding-left:20px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .deposit_td{
	padding-left:60px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .inquire_td .inquire {
    background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -182px;
    border: 0 none;
    color: #000000;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 27px;
	margin-right:20px;
    text-align: center;
    width: 86px;
	}

.withdrawDeposit_record .withdrawDeposit_record_top table .inquire_td .inquire:hover{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -209px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .inquire_td .inquire:active{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -236px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .deposit_td .recharge{
    background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1168px;
    border: 0;
    color: #ffffff;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 16px;
    height: 32px;
    text-align: center;
    width: 116px;
	}

.withdrawDeposit_record .withdrawDeposit_record_top table .deposit_td .recharge:hover{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1200px;
	}
.withdrawDeposit_record .withdrawDeposit_record_top table .deposit_td .recharge:active{
	background:url(../../../../public/images/buttonGroup01.png) no-repeat 0 -1136px;
	color: #000000;
	}
	
	
	

.withdrawDeposit_cont{
	width:1180px;
	/*min-height:75px;*/
	}
.withdrawDeposit_cont .withdrawDeposit_cont_char{
	width:700px;
	text-align:left;
	margin:20px auto 0;
	display:none;
	}
.withdrawDeposit_cont .withdrawDeposit_cont_char .count{
	margin-left:20px;
	}	

	
.withdrawDeposit_cont .withdrawDeposit_cont_char .gobackA{
	color: #0084C7;
    text-decoration: underline;
	margin-left:100px;
	}

	
	
	
		
.withdrawDeposit_cont table{
	margin:20px auto 0;
	border:1px solid #e7e7e5;
	text-align:center;
	color:#676767;
	}
.withdrawDeposit_cont table td{
	padding:8px 15px;
	border:1px solid #e7e7e5;
	}
.withdrawDeposit_cont table th{
	height:30px;
	font-weight:normal;
	background:#cfe69d;
	}
.withdrawDeposit_cont table .td_two .time,
.withdrawDeposit_cont table .td_three .weihao{
	margin-left:10px;
	}
.withdrawDeposit_cont table .td_four,
.withdrawDeposit_cont table .td_five{
	padding:8px 40px;
	}
	
.withdrawDeposit_cont table .td_five .success{
	color:#89abd9;
	}	
.withdrawDeposit_cont table .td_five .check{
	color:#f59701;
	}
.withdrawDeposit_cont table .td_five .div_fail{
	position:relative;
	top:0;
	left:0;
	}	
.withdrawDeposit_cont table .td_five .div_fail .fail_reason{
	position:absolute;
	top:-9px;
	left: -130px;
	width:100px;
	min-height:50xp;
	border:1px solid #E7E7E5;
	background:#fff;
	padding:15px;
	text-align:left;
	z-index:6000;
	display:none;
	}	
.withdrawDeposit_cont table .td_five .div_fail .fail_reason .tit{
	color: #848484;
	height:20px;
    font-family: '微软雅黑';
	}	

.withdrawDeposit_cont table .td_five .div_fail .fail_reason .sanjiao {
	background:url(../../../../public/images/bubbleTriangleRight.png) 0 0 no-repeat;
    height: 10px;
    overflow: hidden;
    position: absolute;
    right: -10px;
    top: 10px;
    width: 10px;
	}	
	
	
.withdrawDeposit_cont table .td_five .fail{
	color:#b4b4b4;
	text-decoration:underline;
	}	

.ppointPaging{
	background: #fff;
	}








