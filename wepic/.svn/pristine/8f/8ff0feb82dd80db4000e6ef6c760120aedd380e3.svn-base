#divAccountHead{
	background:#FFFFFF;
    height: 751px;
    position: relative;
    top: 0;
    left: 0;
    margin:0 auto;
    width: 870px;
	margin-bottom:20px;
	margin-top:10px;
	
	}

#divAccountHead .title{
	position:relative;
	top:0px;
	left:0px;
	height:52px;
	line-height:52px;
	border-bottom:1px solid #f0f0f0;
	text-align:left;
	padding-left:30px;
	}
#divAccountHead .title h2{
	font-size:13px;
	color:#4a4a4a;
	font-weight:normal;
	}








#divAccountHead .conennt{
	position:relative;
	top:0px;
	left:0px;
	height:593px;
	width: 870px;
	border-bottom:1px solid #f0f0f0;
	}
#divAccountHead .conennt .option{
	position:absolute;
	top:0px;
	right:0px;
	height:72px;
	width: 770px;
	text-align:left;
	}	
	
#divAccountHead .conennt input{
     border: 0;
    cursor: pointer;
    font-family: "微软雅黑";
    font-size: 17px;
    height: 32px;
    padding-bottom: 2px;
    text-align: center;
	background: url(../../../../public/images/buttonGroup01.png) no-repeat  0 -1072px;
	}
