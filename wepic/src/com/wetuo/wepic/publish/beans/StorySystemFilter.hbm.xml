<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class 
	name="com.wetuo.wepic.publish.beans.StorySystemFilterWords"
	table="storyfilterwords"
>
	<id
		name="id"
		type="java.lang.Integer"
		column="id"
	>
		<generator class="native" />
	</id>
	
	<property 
		name="words"
		type="java.lang.String"
		column="words"
		length="3000"
	/>
	
	<property 
		name="remark"
		type="java.lang.String"
		column="remark"
		length="3000"
	/>
	
	
</class>
</hibernate-mapping>
