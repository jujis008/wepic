<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<!-- Add packages here -->
	<package name="chat" extends="larry-default" namespace="/">
		<action name="chat*" class="chatAction">
		<result name="travel">/view/chat/travel.jsp</result>
		<result name="traveler">/view/chat/traveler.jsp</result>
		<result name="service">/view/chat/customer.jsp</result>
		</action>
		<action name="access*" class="storAccessInfoAction">
		   <result name="countOnline">userOnline.jsp</result>
		</action>
		
	</package>
</struts>
